{
  "name": "@workspace/auth",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./orm": "./src/orm.ts",
    "./schema": "./src/schema.ts"
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "db:check": "drizzle-kit check --config=src/drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=src/drizzle.config.ts",
    "db:migrate": "tsx ./src/migrate.ts"
  },
  "devDependencies": {
    "dotenv": "^17.2.1"
  },
  "dependencies": {
    "@workspace/db": "workspace:*"
  }
}
